#Riley methods
S3method(print, riley)
S3method(summary, riley)
S3method(print, summary.riley)
S3method(vcov, riley)
S3method(logLik, riley)
S3method(plot, riley)
S3method(riley, default)

#uvmeta methods
S3method(print, uvmeta)
